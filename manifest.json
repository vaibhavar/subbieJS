{
    "manifest_version": 2,
    "name": "Subbie",
    "version": "1.0",
    "permissions": [
          "contextMenus"
        ],
    "background": {
        "scripts": ["background.js"]
    },
    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*"],
            "js": [
                "jquery-3.1.1.min.js", "jquery-ui.min.js","content.js"
            ],
            "css": ["jquery-ui.min.css", "subbie.css"]
        }
    ]
}